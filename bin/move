#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
my_board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe"
puts "Where would you like to go?"
temp1 = gets.strip
puts "What character would you like to use?"
temp2 = gets.strip

move(my_board, temp1, temp2)
